{
    "version": 2,
    "functions": {
        "api": {
            "memory": 1024,
            "maxDuration": 30
        }
    },
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}